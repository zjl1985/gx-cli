{
  "name": "<%= dasherize(name) %>-ng",
  "version": "1.0.0",
  "description": "",
  "author": "zhangjunlong",
  "engines": {
    "node": ">= 8.10.0",
    "npm": ">= 3.0.0"
  },
  "license": "MIT",
  "keywords": [
    "delon",
    "antd",
    "ng-zorro-antd",
    "angular",
    "component",
    "scaffold"
  ],
  "scripts": {
    "precommit": "npm run lint-staged",
    "ng": "ng",
    "start": "ng serve -o --proxy-config proxy.config.json",
    "hmr": "ng serve -c=hmr -o --proxy-config proxy.config.json",
    "build": "ng build  --base-href=. --prod --build-optimizer",
    "analyze": "ng build --prod --build-optimizer --stats-json",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "tslint -p src/tsconfig.app.json -c tslint.json 'src/**/*.ts'",
    "lint:style": "stylelint \"{src}/**/*.less\" --syntax less",
    "lint-staged": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "e2e": "ng e2e",
    "test": "ng test --watch",
    "test-coverage": "ng test --code-coverage --watch=false",
    "package": "ng run my-module:build",
    "pub": "npm --registry http://172.72.100.37:13420/repository/gaoxin-npm/ publish dist"
  },
  "browserslist": [
    "last 2 versions",
    "not ie <= 10",
    "not ie_mob <= 10"
  ],
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
