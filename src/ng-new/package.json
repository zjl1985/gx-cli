    "tslint-language-service": "^0.9.9",
    "typescript": "~3.2.2",
    "webpack-bundle-analyzer": "^3.0.3",
    "xlsx": "^0.14.1"
  },
  "lint-staged": {
    "*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "editorconfig-tools fix",
      "git add"
    ],
    "*.ts": [
      "npm run lint:ts",
      "prettier --write",
      "git add"
    ],
    "src/**/*.less": [
      "npm run lint:style",
      "prettier --write",
      "git add"
    ]
  }
}
