    "gh-pages": "^1.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^7.1.0",
    "mockjs": "^1.0.1-beta3",
    "ng-alain": "^7.5.0",
    "prettier": "^1.15.1",
    "prettier-stylelint": "^0.4.2",
    "protractor": "~5.3.0",
