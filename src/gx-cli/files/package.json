{
    "name": "<%= utils.dasherize(name) %>",
    "version": "0.0.0",
    "description": "重点用能在线监测企业端软件",
    "author": "zhangjunlong",
    "engines": {
        "node": ">= 8.10.0",
        "npm": ">= 3.0.0"
    },
    "license": "MIT",
    "keywords": [
        "delon",
        "antd",
        "ng-zorro-antd",
        "angular",
        "component",
        "scaffold"
    ],
    "scripts": {
        "ng": "ng",
        "start": "ng serve -o --proxy-config proxy.config.json",
        "hmr": "ng serve -c=hmr -o --proxy-config proxy.config.json",
        "build": "ng build  --base-href=. --prod --build-optimizer",
        "analyze": "ng build --prod --build-optimizer --stats-json",
        "lint": "npm run lint:ts && npm run lint:style",
        "lint:ts": "tslint -p src/tsconfig.app.json -c tslint.json 'src/**/*.ts'",
        "lint:style": "stylelint \"{src}/**/*.less\" --syntax less",
        "lint-staged": "lint-staged",
        "tslint-check": "tslint-config-prettier-check ./tslint.json",
        "e2e": "ng e2e",
        "test": "ng test --watch",
        "test-coverage": "ng test --code-coverage --watch=false",
        "tar": "tar -czf dist.tar.gz dist"
    },
    "browserslist": [
        "last 2 versions",
        "not ie <= 10",
        "not ie_mob <= 10"
    ],
    "dependencies": {
        "@angular/animations": "~7.2.0",
        "@angular/common": "~7.2.0",
        "@angular/compiler": "~7.2.0",
        "@angular/core": "~7.2.0",
        "@angular/forms": "~7.2.0",
        "@angular/http": "~7.2.0",
        "@angular/platform-browser": "~7.2.0",
        "@angular/platform-browser-dynamic": "~7.2.0",
        "@angular/router": "~7.2.0",
        "@antv/data-set": "^0.8.8",
        "@antv/g2": "^3.0.11",
        "@antv/g2-plugin-slider": "^2.0.2",
        "@delon/abc": "^7.0.0-rc.6",
        "@delon/acl": "^7.0.0-rc.6",
        "@delon/auth": "^7.0.0-rc.6",
        "@delon/cache": "^7.0.0-rc.6",
        "@delon/chart": "^7.0.0-rc.6",
        "@delon/form": "^7.0.0-rc.6",
        "@delon/mock": "^7.0.0-rc.6",
        "@delon/theme": "^7.0.0-rc.6",
        "@delon/util": "^7.0.0-rc.6",
        "@ngx-translate/core": "^11.0.1",
        "@ngx-translate/http-loader": "^4.0.0",
        "ajv": "^6.6.1",
        "core-js": "^2.5.4",
        "file-saver": "^2.0.0",
        "font-awesome": "^4.7.0",
        "moment": "^2.22.2",
        "ng-zorro-antd": "^7.0.0-rc.3",
        "ngx-countdown": "^3.0.0",
        "ngx-tinymce": "^2.0.0",
        "ngx-ueditor": "^2.0.0",
        "qrious": "^4.0.2",
        "rxjs": "^6.3.3",
        "screenfull": "^3.3.1",
        "simple-line-icons": "^2.4.1",
        "zone.js": "^0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.12.0",
        "@angular/cli": "~7.2.0",
        "@angular/compiler-cli": "~7.2.0",
        "@angular/language-service": "~7.2.0",
        "@angularclass/hmr": "^2.1.3",
        "@types/jasmine": "~2.8.6",
        "@types/jasminewd2": "~2.0.3",
        "@types/jszip": "^3.1.3",
        "@types/mockjs": "^1.0.2",
        "@types/node": "~8.9.4",
        "codecov": "^3.0.2",
        "codelyzer": "~4.2.1",
        "editorconfig-tools": "^0.1.1",
        "gh-pages": "^1.1.0",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~1.7.1",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~1.4.2",
        "karma-jasmine": "~1.1.1",
        "karma-jasmine-html-reporter": "^0.2.2",
        "lint-staged": "^7.1.0",
        "mockjs": "^1.0.1-beta3",
        "ng-alain": "^7.0.0-rc.6",
        "prettier": "^1.15.3",
        "prettier-stylelint": "^0.4.2",
        "protractor": "~5.3.0",
        "stylelint": "^9.2.0",
        "stylelint-config-standard": "^18.2.0",
        "ts-node": "~5.0.1",
        "tslint": "~5.9.1",
        "tslint-config-prettier": "^1.17.0",
        "tslint-language-service": "^0.9.9",
        "typescript": "~3.2.2",
        "webpack-bundle-analyzer": "^3.0.3",
        "xlsx": "^0.14.1"
    },
    "lint-staged": {
        "*.{cmd,html,json,md,sh,txt,xml,yml}": [
            "editorconfig-tools fix",
            "git add"
        ],
        "*.ts": [
            "npm run lint:ts",
            "prettier --write",
            "git add"
        ],
        "src/**/*.less": [
            "npm run lint:style",
            "prettier --write",
            "git add"
        ]
    }
}
